{
    "name": "mastoshare",
    "description": "Mastoshare For Firefox",
    "version": "1.1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "foreground/popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "storage"
    ]
}